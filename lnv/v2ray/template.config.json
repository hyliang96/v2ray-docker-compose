{
  "log": {
    "loglevel": "warning",
    "access": "/etc/v2ray/log/access.log",
    "error": "/etc/v2ray/log/error.log"
  },
  "inbounds": [
    {
      "port": 10001,
      "protocol": "vmess",
      "settings": {
        "clients": [
          {
            "id": "UUID",
            "alterId": 64
          }
        ]
      },
    "streamSettings": {
      "network": "ws",
      "wsSettings": {
        "path": "/your_v2ray_path"
        }
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "settings": {}
    }
  ]
}
